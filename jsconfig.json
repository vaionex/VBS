{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "lib": ["es2015", "dom"],
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/config/*": ["src/config/*"],
      "@/services/*": ["src/services/*"],
      "@/constants/*": ["src/constants/*"],
      "@/redux/*": ["src/redux/*"],
      "@/styles/*": ["src/styles/*"],
      "@/images/*": ["src/images/*"],
      "@/firebase/*": ["src/firebase/*"],
      "@/utils/*": ["src/utils/*"],
      "@/hooks/*": ["src/hooks/*"]
    }
  }
}
